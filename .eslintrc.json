{
  "extends": ["react-app", "prettier"],
  "rules": {
    "max-len": ["warn", { "code": 120 }],
    "linebreak-style": "off",
    "react/destructuring-assignment": [false],
    "react/prefer-stateless-function": "off",
    "react/prop-types": "off",
    "no-unused-vars": ["warn", { "args": "none" }],
    "import/prefer-default-export": "off",
    "import/order": "off"
  },
  "env": {
    "browser": true
  }
}
