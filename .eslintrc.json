{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "linebreak-style": "off",
    "react/destructuring-assignment": [0],
    "react/prefer-stateless-function": "off",
    "no-unused-vars": ["error", { "args": "none" }]
  },
  "env": {
    "browser": true
  }
}
