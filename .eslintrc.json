{
  "extends": "airbnb",
  "rules": {
    "linebreak-style": "off",
    "react/destructuring-assignment": [false],
    "react/prefer-stateless-function": "off",
    "react/prop-types": "off",
    "no-unused-vars": ["error", { "args": "none" }],
    "import/prefer-default-export": "off"
  },
  "env": {
    "browser": true
  }
}
